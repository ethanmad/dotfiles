"/usr/bin/amixer -q sset Master 5+ && /home/ethan/bin/notifyvolume"
    XF86AudioRaiseVolume

"/usr/bin/amixer -q sset Master 5- && /home/ethan/bin/notifyvolume"
    XF86AudioLowerVolume

"/usr/bin/amixer -q sset Master 0% && /home/ethan/bin/notifyvolume"
    XF86AudioMute

"sudo ~/linux-4.1-samus/scripts/setup/brightness --decrease"
    Control+F6
